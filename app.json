{
  "expo": {
    "name": "yunvd",
    "slug": "yunvd",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "NSCameraUsageDescription": "This app uses the camera to allow you to upload video content.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record audio for videos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to allow you to upload existing videos."
      },
      "bundleIdentifier": "com.zyun.yunvd"
    },
    "android": {
      "splash": {
        "image": "./assets/icon-1920x720.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ],
      "package": "com.zyun.yunvd"
    },
    "plugins": [
      "@bam.tech/react-native-keyevent-expo-config-plugin",
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/icon-400x240.png",
          "appleTVImages": {
            "icon": "./assets/icon-1280x768.png",
            "iconSmall": "./assets/icon-400x240.png",
            "iconSmall2x": "./assets/icon-800x480.png",
            "topShelf": "./assets/icon-1920x720.png",
            "topShelf2x": "./assets/icon-1920x720.png",
            "topShelfWide": "./assets/icon-1920x720.png",
            "topShelfWide2x": "./assets/icon-1920x720.png"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b438e72f-8c15-47da-890f-c23695300a6e"
      }
    }
  }
}
