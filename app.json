{
  "expo": {
    "name": "yunvd",
    "slug": "yunvd",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "NSCameraUsageDescription": "This app uses the camera to allow you to upload video content.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record audio for videos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to allow you to upload existing videos."
      },
      "bundleIdentifier": "com.zyun.yunvd"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ],
      "package": "com.zyun.yunvd"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@bam.tech/react-native-keyevent-expo-config-plugin",
      [
        "@react-native-tvos/config-tv",
        {
          "showVerboseWarnings": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b438e72f-8c15-47da-890f-c23695300a6e"
      }
    }
  }
}
